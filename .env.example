# ====================================
# SUPABASE CONFIGURATION
# ====================================

# Пароль для PostgreSQL базы данных
SUPABASE_DB_PASSWORD=your_secure_postgres_password_here

# JWT токены для аутентификации
# Сгенерируйте с помощью: openssl rand -base64 32
SUPABASE_JWT_SECRET=your_jwt_secret_at_least_32_characters_here

# API ключи
# Сгенерируйте JWT токены для этих ключей
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Домены (опционально)
SUPABASE_API_EXTERNAL_URL=https://api.supabase.yourdomain.com
SUPABASE_STUDIO_URL=https://supabase.yourdomain.com

# Email настройки для отправки писем
SUPABASE_SMTP_HOST=smtp.your-mail-provider.com
SUPABASE_SMTP_PORT=587
SUPABASE_SMTP_USER=your-email@example.com
SUPABASE_SMTP_PASS=your-smtp-password
SUPABASE_SMTP_SENDER_NAME=Supabase

# ====================================
# N8N CONFIGURATION
# ====================================

# Пароль для PostgreSQL БД n8n
N8N_DB_PASSWORD=your_n8n_database_password_here

# Ключ шифрования для n8n
# Сгенерируйте с помощью: openssl rand -base64 32
N8N_ENCRYPTION_KEY=your_n8n_encryption_key_at_least_32_characters_here

# Веб-адрес n8n
N8N_HOST=0.0.0.0
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_DOMAIN=n8n.yourdomain.com

# Webhook URL
WEBHOOK_URL=https://n8n.yourdomain.com/

# Email настройки для уведомлений
N8N_SMTP_HOST=smtp.your-mail-provider.com
N8N_SMTP_PORT=587
N8N_SMTP_USER=your-email@example.com
N8N_SMTP_PASS=your-smtp-password
N8N_SMTP_SENDER=n8n@yourdomain.com

# Лимиты выполнения
N8N_EXECUTION_TIMEOUT=3600
N8N_EXECUTION_TIMEOUT_WARNING=3000

# ====================================
# PGADMIN CONFIGURATION
# ====================================

PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=your_pgadmin_password_here

# ====================================
# MONITORING & LOGGING
# ====================================

# Уровень логирования: error, warn, info, debug, trace
LOG_LEVEL=info

# Таймовая зона
TIMEZONE=UTC

# ====================================
# BACKUP CONFIGURATION
# ====================================

# Путь для резервных копий
BACKUP_PATH=/backups

# Email для получения отчетов о резервных копиях
BACKUP_EMAIL=admin@example.com

# ====================================
# SECURITY CONFIGURATION
# ====================================

# Пароль для Slack webhook (если используется)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# API ключи для внешних сервисов
EXTERNAL_API_KEY=your_external_api_key_here
